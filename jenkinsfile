#!groovy

properties([disableConcurrentBuilds()])

pipeline {
    agent {
         label 'ubuntu16' 
         }

    stages {
        stage('Build') {
            steps {
                echo 'Building..' 
                
                sh 'cmake cpp/example1/ '
                    
                sh 'make'
              
                sh './main'
                
            }
        }
        stage('constructing a package') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}